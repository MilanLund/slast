extends includes/layout.pug
block content
    main(id='main')
        section.content
            .grid
                .grid__wrapper
                    .grid__unit.grid__unit--1
                        .logo-art.logo-art--author
                            .logo-art__wrap.logo-art__wrap--logo.logo-art__wrap--small
                                img.logo-art__img.logo-art__img--small(src='./img/logo-art.png', alt='Slast')
                                .logo-art__stripe.logo-art__stripe--small
                                    h1.sr-only Slast
                                    | art.
                            .logo-art__content
                                .logo-art__left
                                    .logo-art__name Jméno díla
                                    p.logo-art__price Cena: 45 000 Kč
                                    a(href='#').logo-art__button Koupit
                                .logo-art__right
                                    p.content__paragraph
                                        | Autor: 
                                        a(href='/art/hana-hrusova') Hana Hrušová
                                        br
                                        | Vystavující ve Slasti: listopad 2018 – květen 2019
                                        br
                                        | Stav díla: Volné
                                    p.content__price Cena: 12.000 Kč
                                    h2.content__heading Mám zájem o dílo
                                    .form-order
                                        #p_lt_ctl00_pageplaceholder_p_lt_WebPartZone1_ZoneA_wD_viewBiz
                                            #p_lt_ctl00_pageplaceholder_p_lt_WebPartZone1_ZoneA_wD_viewBiz_pM_pMP
